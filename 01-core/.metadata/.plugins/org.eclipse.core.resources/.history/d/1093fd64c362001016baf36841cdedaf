package com.java8;


interface Addable{
	
	   int add(int a,int b);
	  
}

interface Substraction{
	
	 int sub(int a,int b);
}


public class java {

	   public static void main(String[] args) 
	   {
		
		   
		   Addable ad1=(a,b)->(a+b);
		   
		   System.out.println(ad1.add(4, 5));
		   
		   Substraction sub=(a,b)->{
			    
			     return a-b;
		   };
		   
		   System.out.println(sub.sub(5, 3));
		   
		
		   
	}
	
}
